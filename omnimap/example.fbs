
attribute "priority";

enum Color : byte { Red = 1, Green, Blue }

struct Vec3 {
  x:float;
  y:float;
  z:float;
}

table mil {
  ju: float;
}

table Monster {
  ulu: mil;
  chango:  [string];

  pos:Vec3;
  mana:short = 150;
  hp:short = 100;
  name:string;
  friendly:bool = false (deprecated, priority: 1);
  inventory:[ubyte];
  color:[Color];
}

root_type Monster;